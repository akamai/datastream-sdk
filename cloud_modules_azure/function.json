{
    "scriptFile": "__init__.py",
    "entryPoint": "main",
    "bindings": [
        {
            "name": "myblob",
            "type": "blobTrigger",
            "direction": "in",
            "path": "data/{name}",
            "connection": "AzureDataStorageConnectionString"
        },
        {
            "type": "cosmosDB",
            "direction": "out",
            "name": "resultdoc",
            "databaseName": "ds2demodb",
            "collectionName": "ds2democontainer",
            "createIfNotExists": "true",
            "connectionStringSetting": "AzureCosmosDBConnectionString"
        }
    ]
}
